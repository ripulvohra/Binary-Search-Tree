#include <bits/stdc++.h>
using namespace std;
struct node
{
 int data;
 node* left;
 node* right;
};


bool isBST(node* root,int min,int max)
{
    if(root==NULL) return true;
    
    if(root->data>min&&root->data<max&&isBST(root->left,min,root->data)&&isBST(root->right,root->data,max))
    return true;
    else false;
}
int main() {
node* root=NULL;
int n,c;
cin>>n;
node* ptr;
for(int i=0;i<n;i++)
    {
       cin>>c;
       node* temp=new node();
       temp->data=c;
       temp->left=NULL;
       temp->right=NULL;
       
       if(root==NULL) root=temp;
       
       else 
       
       {
       ptr=root;
       while(1)
       {
       if(temp->data>=ptr->data)
       {
           if(ptr->right!=NULL)
           ptr=ptr->right;
           else break;
       }
       else
       {
           if(ptr->left!=NULL)
           ptr=ptr->left;
           else break;
       }
       }
    
           if(temp->data>=ptr->data) ptr->right=temp;
           else ptr->left=temp;
       }
    
}
 cout<<isBST(root,INT_MIN,INT_MAX);
}
